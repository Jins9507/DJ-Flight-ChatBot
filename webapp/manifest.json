{
  "_version": "1.59.0",
  "sap.app": {
    "id": "dj.djchatbot",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.13.3",
      "toolsId": "99058f0c-0b41-45ec-836b-bf65b9082a08"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZUI_C_TRAVEL_DJ_010",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "testModel": {
        "uri": "model/testModel.json",
        "type": "JSON"
      },
      "mockUser": {
        "uri": "model/mockUserjson",
        "type": "JSON"
      },
      "filterModel": {
        "uri": "model/filterModel.json",
        "type": "JSON"
      }            
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.123.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "dj.djchatbot.i18n.i18n"
        }
      },
      "testModel": {
        "dataSource": "testModel",
        "type": "sap.ui.model.json.JSONModel"
      },
      "filterModel": {
        "dataSource": "filterModel",
        "type": "sap.ui.model.json.JSONModel"
      },
      "mockUser": {
        "dataSource": "mockUser",
        "type": "sap.ui.model.json.JSONModel"
      },      
      "reserveModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "mainModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
            "defaultBindingMode": "TwoWay",
            "defaultCountMode": "Inline",
            "refreshAfterChange": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "dj.djchatbot.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "pattern": "schedule/{?query}",
          "name": "CheckFlight",
          "target": [
              "TargetCheck"
          ]
        },
        {
          "pattern": "schedule/reservation/{?query}",
          "name": "Reservation",
          "target": [
              "Reservation"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "TargetCheck": {  
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,           
          "viewId": "CheckFlight",      				
					"viewName": "CheckFlight"
        },
        "Reservation": {  
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,           
          "viewId": "Reservation",      				
					"viewName": "Reservation"
        }
      }
    },
    "rootView": {
      "viewName": "dj.djchatbot.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
